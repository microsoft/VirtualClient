        <RootNamespace>VirtualClient</RootNamespace>
        <NoWarn>CA1063;CA2214;CS1591;SA1508</NoWarn>
    </PropertyGroup>

    <ItemGroup>
        <!-- Global package dependency versions are defined in the Module.props for the solution. -->
